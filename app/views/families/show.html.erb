<div class="main">
  <div class="inner">
    <div class="user__wrapper">
      <h2 class="page-heading">
        <%= "#{@family.family_name}さんの情報"%>
      </h2>
      <table class="table">
        <tbody>
          <tr>
            <th class="table__col1">名前</th>
            <td class="table__col2"><%= @family.family_name %></td>
          </tr>
          <tr>
            <th class="table__col1">プロフィール</th>
            <td class="table__col2"><%= @family.email %></td>
          </tr>
          
        </tbody>
      </table>
      <h2 class="page-heading">
        <%= "#{@family.family_name}さんのこども"%>
        <% @users.each do |user| %>
          <p><%= link_to user.nickname, family_user_path(@family, user) %></p>
         <% end %>
      </h2>
        

      <div class="user__card">
        <%# <%= render partial: 'prototypes/prototype', collection: @user.prototypes %>
      </div> 

      <%= link_to 'トップページ', root_path %>
    </div>
  </div>
</div>